{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": ["error","never"],
        "no-console": "off",
        "no-unused-vars": "off"
    },
    "plugins": [
        "react"
    ],
    "globals": {

    }
}